{
  "$schema": "https://dprint.dev/schemas/v0.json",
  "incremental": true,
  "json": {
    "indentWidth": 2,
    "useTabs": false
  },
  "toml": {
    "indentWidth": 2
  },
  "includes": [
    "**/*.json",
    "**/*.toml"
  ],
  "excludes": [
    "**/node_modules",
    "**/vendor",
    "**/dist",
    "**/.git",
    "**/.beads"
  ],
  "plugins": [
    "https://plugins.dprint.dev/json-0.19.3.wasm",
    "https://plugins.dprint.dev/toml-0.6.3.wasm"
  ]
}
